{
  "name": "json-schema-org/json-schema-spec",
  "version": "1.0.0",
  "description": "The JSON Schema Specification",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-core && npm run build-validation",
    "build-core": "node build/build.js < jsonschema-core.md > jsonschema-core.html",
