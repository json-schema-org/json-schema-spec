    "remark": "^14.0.3",
    "remark-flexible-containers": "^1.0.6",
    "remark-gfm": "^3.0.1",
    "remark-preset-lint-markdown-style-guide": "^5.1.3",
    "remark-rehype": "^10.1.0",
    "remark-toc": "^8.0.1",
    "remark-torchlight": "^0.0.5",
    "remark-validate-links": "^12.1.1",
    "vfile-reporter": "^8.0.0"
  },
  "devDependencies": {
    "dotenv": "^16.3.1"
  }
